import numpy as np

def research14(a2):
    dd = np.zeros((2, 2))
    
    dd[0, 0] = a2[0, 0]**2 + 1**2
    dd[1, 0] = a2[1, 0]**2 + 1**2
    dd[0, 1] = a2[0, 1]**2 + 1**2
    dd[1, 1] = a2[1, 1]**2 + 1**2
    
    dd[0, 0] = np.sqrt(dd[0, 0])
    dd[1, 0] = np.sqrt(dd[1, 0])
    dd[0, 1] = np.sqrt(dd[0, 1])
    dd[1, 1] = np.sqrt(dd[1, 1])
    
    return dd.T
