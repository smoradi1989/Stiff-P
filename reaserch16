import numpy as np

def reaserch16(mmm):
    dd = np.zeros(6)
    
    dd[0] = abs(mmm[0, 0]) + mmm[4, 0]
    dd[1] = abs(mmm[1, 0]) + mmm[5, 0]
    dd[2] = abs(mmm[1, 0]) + mmm[3, 0]
    dd[3] = abs(mmm[1, 0]) + mmm[5, 0]
    dd[4] = abs(mmm[2, 0]) + mmm[3, 0]
    dd[5] = abs(mmm[2, 0]) + mmm[4, 0]
    
    return dd
