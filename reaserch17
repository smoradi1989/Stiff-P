import numpy as np

def reaserch17(a4):
    dd = np.zeros((3, 2))
    
    dd[0, 0] = a4[0]**2 + 1**2
    dd[0, 1] = a4[1]**2 + 1**2
    dd[1, 0] = a4[2]**2 + 1**2
    dd[1, 1] = a4[3]**2 + 1**2
    dd[2, 0] = a4[4]**2 + 1**2
    dd[2, 1] = a4[5]**2 + 1**2

    dd[0, 0] = np.sqrt(dd[0, 0])
    dd[0, 1] = np.sqrt(dd[0, 1])
    dd[1, 0] = np.sqrt(dd[1, 0])
    dd[1, 1] = np.sqrt(dd[1, 1])
    dd[2, 0] = np.sqrt(dd[2, 0])
    dd[2, 1] = np.sqrt(dd[2, 1])
    
    return dd
